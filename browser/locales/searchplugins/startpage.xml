<?xml version="1.0" encoding="utf-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
	<ShortName>Startpage</ShortName>
	<Description>Startpage.com Search</Description>
	<InputEncoding>UTF-8</InputEncoding>
	<Image height="16" width="16">data:image/png;base64,</Image>
	<Url type="application/x-suggestions+json" template="https://www.startpage.com/cgi-bin/csuggest?query={searchTerms}&amp;limit=10&amp;lang=english&amp;format=json"/>
	<Url type="text/html" method="POST" template="https://www.startpage.com/do/dsearch">
		<Param name="query" value="{searchTerms}&amp;segment=startpage.waterfox.2"/>
	</Url>
</SearchPlugin>
